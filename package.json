{
  "name": "living-meta-analysis",
  "version": "0.0.1",
  "description": "Living Meta-Analysis tool",
  "scripts": {
    "start": "node server",
    "nodemon": "node_modules/.bin/nodemon -w server/ server",
    "start:test-server": "TESTING=true node server",
    "start:redirected": "REDIRECT_API=https://uop-soc-lima.ew.r.appspot.com TESTING=1 nodemon -w server/ server",
    "db-migrate": "node server/storage/scripts/migrate",
    "create-api-snapshot": "node test/qunit/snapshots/create_snapshot",
    "beta-codes": "node server/storage/scripts/betacodes",
    "test-server": "TESTING=true qunit test/qunit/tests/*-test.js",
    "lint-server": "echo eslint && eslint server/** --ext .js,.json",
    "lint": "echo eslint && eslint . --ext .js,.json && echo stylelint && stylelint webpages*/**/*.css webpages*/**/*.html",
    "build": "parcel build --no-cache --public-url /react-dist/ --dist-dir webpages/react-dist/ webpages-react/metaanalysis.html",
    "react-watch": "NODE_ENV=development parcel watch --no-cache --public-url /react-dist/ --dist-dir webpages/react-dist/ webpages-react/metaanalysis.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacekkopecky/living-meta-analysis.git"
  },
  "author": "Jacek Kopecky <jacek.kopecky@port.ac.uk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacekkopecky/living-meta-analysis/issues"
  },
  "homepage": "https://github.com/jacekkopecky/living-meta-analysis#readme",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "ajv": "^6.12.2",
    "ajv-errors": "^1.0.1",
    "ajv-merge-patch": "^4.1.0",
    "babel-eslint": "^10.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-portsoc": "^0.6.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react-hooks": "^4.0.2",
    "eslint-plugin-react": "^7.20.0",
    "eslint": "^7.0.0",
    "istanbul": "^0.4.5",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.3",
    "qunit": "^2.10.0",
    "parcel": "^2.0.0-beta.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react": "^16.13.1",
    "require-directory": "^2.1.1",
    "selenium-standalone": "^6.17.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint": "^13.4.0"
  },
  "dependencies": {
    "@google-cloud/datastore": "^5.1.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "simple-google-openid": "^0.3.0",
    "uid": "^1.0.0"
  }
}
